<%- include('../partials/head') %>

<div class="container">

    <h1>Editing <%= listing.streetAddress %></h1>

    <form action="/listings/<%=user._id%>/<%=listing._id%>/?_method=PUT" method="POST">
        <label for="streetAddress">Street Address:</label>
        <input type="text" name="streetAddress" value="<%= listing.streetAddress %>">
        <label for="city">city:</label>
        <input type="text" name="city" value="<%= listing.city %>">
        <label for="price">price:</label>
        <input type="number" name="price" id="price" value="<%= listing.price %>"></input>
        <label for="size">size:</label>
        <input type="text" name="size" value="<%= listing.city %>">
        <label for="size">image:</label>
        <input type="text" name="imageUrl" value="<%= listing.imageUrl %>">
        <button type="submit" class="btn btn-primary">Add Listing</button>
    </form>


</div>


<%- include('../partials/footer') %>
