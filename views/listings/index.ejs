<%- include('../partials/head') %>

<div class="container">

    <h1>Here's all the listings</h1>

<% listings.forEach(listing => { %>
  
    <div class="card" style="width: 18rem;">
        <img src="<%= listing.imageUrl %>" class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title">Price:$<%= listing.price %></h5>
          <p class="card-text">Street Address :<%= listing.streetAddress %></p>
          <a href="listings/<%= listing._id %>" class="btn btn-primary">View Details</a>
        </div>
      </div>
  



<% }) %>


</div>


<%- include('../partials/footer') %>
